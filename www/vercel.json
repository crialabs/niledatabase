{
  "rewrites": [
    {
      "source": "/docs",
      "destination": "https://nile.mintlify.dev/docs/"
    },
    {
      "source": "/docs/",
      "destination": "https://nile.mintlify.dev/docs/"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://nile.mintlify.dev/docs/:match*"
    },
    {
      "source": "/docs/*",
      "destination": "https://nile.mintlify.dev/docs/:match*"
    }
  ]
}
